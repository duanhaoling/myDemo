        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".webview.WebViewDemo" />
        <activity android:name=".webview.WebViewDemo2" />
        <activity android:name=".volley.VolleyActivity" />
        <activity
            android:name=".adapter.MyListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".timer.TimerDemoActivity" />
        <activity android:name=".animation.AnimationActivity" />
        <activity
            android:name=".widget.test.WidgetActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".material.CoordActivity"
            android:theme="@style/CoordinatorStyle" />
        <activity android:name=".popu.LiuyanActivity" />
        <activity
            android:name=".viewflipper.ViewFlipperActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
